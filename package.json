{
  "name": "app",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "start": "bun run src/index.ts",
    "start:prod": "bun run dist/index.js",
    "routes:gen": "bun run scripts/generate-routes.ts",
    "export-postgres": "tsx scripts/export-from-postgres.ts",
    "import-mock-data": "tsx scripts/import-to-mock-data.ts",
    "test-db": "tsx scripts/test-db-connection.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "echo \"Linting not configured yet\"",
    "format": "bun run --prettier src/**/*.ts"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.3.0",
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/jwt": "^1.3.2",
    "@elysiajs/swagger": "^1.3.1",
    "@types/jsonwebtoken": "^9.0.10",
    "elysia": "latest",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "uuid": "^11.1.0",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "bun-types": "latest",
    "tsx": "^4.20.5"
  },
  "module": "src/index.js"
}
